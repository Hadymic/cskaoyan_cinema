<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cskaoyan.cinema.rest.common.persistence.dao.YearDictTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cskaoyan.cinema.rest.common.persistence.model.YearDictT">
        <id column="UUID" property="uuid" />
        <result column="show_name" property="showName" />
    </resultMap>

    <select id="selectAll" resultType="com.cskaoyan.cinema.rest.common.persistence.vo.YearInfoVo">
        select UUID as yearId, show_name as yearName
        from mtime_year_dict_t
    </select>
</mapper>
