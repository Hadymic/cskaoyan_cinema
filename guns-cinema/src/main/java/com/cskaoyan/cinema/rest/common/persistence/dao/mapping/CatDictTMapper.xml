<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cskaoyan.cinema.rest.common.persistence.dao.CatDictTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cskaoyan.cinema.rest.common.persistence.model.CatDictT">
        <id column="UUID" property="uuid" />
        <result column="show_name" property="showName" />
    </resultMap>
    <select id="selectListByUUID" resultType="com.cskaoyan.cinema.vo.BrandVo">
      select UUID as brandId,show_name as brandName from `mtime_cat_dict_t`
      <if test="brandId != 99">
          where UUID = #{brandId}
      </if>
    </select>
</mapper>
