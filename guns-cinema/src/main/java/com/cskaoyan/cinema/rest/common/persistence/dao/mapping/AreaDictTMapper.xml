<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cskaoyan.cinema.rest.common.persistence.dao.AreaDictTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cskaoyan.cinema.rest.common.persistence.model.AreaDictT">
        <id column="UUID" property="uuid" />
        <result column="show_name" property="showName" />
    </resultMap>
    <select id="selectListByUUID" resultType="com.cskaoyan.cinema.vo.cinema.AreaVo">
        select UUID as areaId,show_name as areaName from `mtime_area_dict_t`
        <!--<if test="areaId != 99">-->
            <!--where UUID = #{areaId}-->
        <!--</if>-->
    </select>
</mapper>
