<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cskaoyan.cinema.rest.common.persistence.dao.HallDictTMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cskaoyan.cinema.rest.common.persistence.model.HallDictT">
        <id column="UUID" property="uuid" />
        <result column="show_name" property="showName" />
        <result column="seat_address" property="seatAddress" />
    </resultMap>
    <select id="selectListByUUID" resultType="com.cskaoyan.cinema.vo.HalltypeVo">
        select UUID as  hallType,show_name as halltypeName from `mtime_hall_dict_t`
        <if test="hallType != 99">
            where UUID = #{hallType}
        </if>
    </select>
</mapper>
